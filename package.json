{
  "name": "pdf-summary-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test  jest",
    "build": "rimraf ./dist && tsc && tsc-alias",
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon --exec \"ts-node -r tsconfig-paths/register\" index.ts",
    "lint": "eslint . --ext .ts",
    "format": "eslint . --ext .ts --fix",
    "prettier": "prettier --write .",
    "lint:fix": "npm run format && npm run lint",
    "migration": "npm run typeorm migration:run -- -d ./src/database/index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "typeorm:sync": "npm run typeorm schema:sync -- -d ./src/database/index.ts",
    "typeorm:drop": "npm run typeorm schema:drop",
    "typeorm:reset": "npm runde typeorm:drop && yarn typeorm:sync",
    "migrate": "npm run typeorm migration:generate ./src/migrations -- -d ./src/database/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/community": "^0.2.4",
    "@langchain/google-genai": "^0.0.16",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.2.3",
    "memory-cache": "^0.2.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "tsc-alias": "^1.8.10",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/memory-cache": "^0.2.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.13",
    "@types/pdf-parse": "^1.1.4",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
